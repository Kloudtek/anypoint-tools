{
  "apis": [
    {
      "name": "Test",
      "version": "1.0.0",
      "endpoint": "${url}/xxx",
      "access": [
        {
          "name": "testapp",
          "version": "v1"
        }
      ],
      "policies": [
        {
          "type": "client-id-enforcement",
          "clientIdExpr": "#[message.inboundProperties[\"client_id\"]]",
          "clientSecretExpr": "#[message.inboundProperties[\"client_secret\"]]"
        },
        {
          "type": "json",
          "data": [
            {
              "itemName": "Default configuration",
              "itemData": {
                "message": "#['foo']",
                "level": "INFO",
                "firstSection": true,
                "secondSection": true
              }
            }
          ]
        }
      ]
    }
  ]
}
